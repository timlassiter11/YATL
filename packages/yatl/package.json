{
  "name": "@timlassiter11/yatl",
  "version": "1.2.2",
  "description": "Yet another table library with all of the standard bells and whistles.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/yatl.min.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./theme.css": "./dist/theme.css"
  },
  "sideEffects": [
    "./dist/index.mjs",
    "./dist/index.js"
  ],
  "keywords": [
    "table",
    "datatable",
    "typescript",
    "javascript",
    "library"
  ],
  "author": "Timothy Lassiter <timlassiter11@gmail.com> (https://github.com/timlassiter11/YATL)",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "type:check": "npx tsc --noEmit",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timlassiter11/YATL.git"
  },
  "bugs": {
    "url": "https://github.com/timlassiter11/YATL/issues"
  },
  "homepage": "https://github.com/timlassiter11/YATL#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.17",
    "@vitest/browser-playwright": "^4.0.18",
    "eslint": "^9.28.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.13",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^4.0.18",
    "vitest-browser-lit": "^1.0.1"
  },
  "dependencies": {
    "@lit-labs/virtualizer": "^2.1.1",
    "lit": "^3.3.2"
  }
}
