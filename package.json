{
  "name": "@timlassiter11/yatl-mono",
  "private": true,
  "workspaces": [
    "packages/yatl",
    "packages/yatl-ui",
    "packages/playground"
  ],
  "scripts": {
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "format": "npm run format --workspaces --if-present",
    "format:check": "npm run format:check --workspaces --if-present",
    "type:check": "npm run type:check --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "build:table": "npm run build --workspace=packages/yatl",
    "build:watch:table": "npm run build:watch --workspace=packages/yatl",
    "build:ui": "npm run build --workspace=packages/yatl-ui",
    "build:watch:ui": "npm run build:watch --workspace=packages/yatl-ui",
    "build:playground": "npm run build --workspace=packages/playground",
    "dev:playground": "npm run dev --workspace=packages/playground"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8"
  },
  "version": ""
}
