{
  "name": "@timlassiter11/yatl",
  "version": "1.0.0",
  "description": "Yet another table library with all of the standard bells and whistles.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/yatl.min.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": [
    "./dist/index.mjs",
    "./dist/index.js"
  ],
  "keywords": [
    "table",
    "datatable",
    "typescript",
    "javascript",
    "library"
  ],
  "author": "Timothy Lassiter <timlassiter11@gmail.com> (https://github.com/timlassiter11/YATL)",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint",
    "format": "prettier src/* --write",
    "build": "tsup",
    "test": "web-test-runner src/**/*.test.ts --node-resolve",
    "test:watch": "web-test-runner src/**/*.test.ts --node-resolve --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timlassiter11/YATL.git"
  },
  "bugs": {
    "url": "https://github.com/timlassiter11/YATL/issues"
  },
  "homepage": "https://github.com/timlassiter11/YATL#readme",
  "engines": {
    "node": ">22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@open-wc/testing": "^4.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.17",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.4",
    "@web/test-runner": "^0.20.2",
    "esbuild": "^0.25.4",
    "eslint": "^9.28.0",
    "prettier": "^3.5.3",
    "sinon": "^21.0.1",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.13",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@lit-labs/virtualizer": "^2.1.1",
    "lit": "^3.3.2"
  }
}